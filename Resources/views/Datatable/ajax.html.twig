{% if view_features.serverSide %}
    "serverSide": true,
    "ajax": {
        "url": "{{ view_ajax.url|raw }}",
        "type": "{{ view_ajax.type }}"
    },
{% else %}
    "serverSide": false,
    "data": {{ view_data|raw }},
{% endif %}
